#![enable(implicit_some)]
Prefab(
    entities: [
    // Grey bat
        PrefabEntity(
            data: Object(
            // SpriteScenePrefab
                sprite_scene: (
                // SpriteSheetPrefab with index 0
                    sheet: Sheet(
                    // TexturePrefab
                        texture: File("image/bat.png", (
                            "IMAGE", (
                                sampler_info: (
                                    min_filter: Nearest,
                                    mag_filter: Nearest,
                                    mip_filter: Nearest,
                                    wrap_mode: (Tile, Tile, Tile),
                                    lod_bias: (0),
                                    lod_range: (
                                        start: ( 0 ),
                                        end: ( 8000 ),
                                    ),
                                    comparison: None,
                                    border: (0),
                                    anisotropic: Off,
                                ),
                            ),
                        )),
                        sprites: [
                        // Grey bat sprites
                        // Sprites with indexes from 0 to 5
                            Grid((
                                     texture_width: 192,
                                     texture_height: 64,
                                     columns: 6,
                                     rows: 1,
                                     cell_size: (32, 32),
                                 // Shifts the grid by 32 pixels down and the bat changes to brown
                                 // position: (0, 32),
                            ))
                        ],
                        // optional
                        name: "bat",
                    ),
                    // SpriteRenderPrefab
                    render: (
                        sheet: "bat",
                        sprite_number: 1,
                    ),
                    // Transform
                    transform: (
                        translation: (200.0, 200.0, 0.0),
                    )
                ),
                // AnimationSetPrefab
                animation_set: (
                    animations: [
                        (
                        // AnimationId
                            Fly,
                            (
                                samplers: [
                                    (
                                        0,
                                        // Only SpriteIndex channel allowed for SpriteRender in AnimationSetPrefab
                                        SpriteIndex,
                                        (
                                            // Time of key frames
                                            input: [0.0, 0.5, 0.6, 0.7, 0.8, 0.9, 1.0, 1.1, 1.2, 1.3, 1.4],
                                            // Sprite indexes from SpriteSheet for key frames
                                            output: [5, 4, 3, 2, 1, 0, 1, 2, 3, 4, 4],
                                            // Sprites can only ever be animated with Step
                                            function: Step,
                                        )
                                    )
                                ]
                            )
                        )
                    ]
                )
            )
        )
    ]
)